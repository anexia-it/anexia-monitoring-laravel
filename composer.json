{
    "name": "anexia/laravel-monitoring",
    "version": "1.0",
    "autoload": {
        "psr-4": { "Anexia\\Monitoring\\": "src" }
    },
    "require": {
        "composer/semver": "~1.4.2"
    },
    "scripts": {
        "post-install-cmd": [
            "php artisan vendor:publish --provider=\"Anexia\\Monitoring\\Providers\\MonitoringServiceProvider\""
        ],
        "post-update-cmd": [
            "php artisan vendor:publish --provider=\"Anexia\\Monitoring\\Providers\\MonitoringServiceProvider\""
        ]
    }
}